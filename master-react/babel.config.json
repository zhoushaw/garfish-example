{
    "presets": [
        "@babel/preset-typescript",
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": "3.6.5",
                "targets": {
                    "chrome": "48",
                    "ie": "11"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties"
    ]
}
